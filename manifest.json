{
  "manifest_version": 3,
  "name": "To-Md",
  "version": "1.0",
  "permissions": ["activeTab", "downloads", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "128": "./icons/icon128.png",
      "48": "./icons/icon48.png",
      "512": "./icons/icon512.png"
    },
    "default_title": "to Markdown"
  },
  "icons": {
    "128": "./icons/icon128.png",
    "48": "./icons/icon48.png",
    "512": "./icons/icon512.png"
  }
}